<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/mediumAshblue</item>
        <item name="colorPrimaryDark">@color/lightAshblue</item>
        <item name="colorAccent">@color/mediumLime</item>
    </style>
    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="RatingBar" parent="Theme.AppCompat">
        <item name="colorControlNormal">@color/basicWhite</item>
        <item name="colorControlActivated">@color/mediumYellow</item>
    </style>
    <style name="RatingBar2" parent="Theme.AppCompat">
        <item name="colorControlNormal">@color/lightAshblue</item>
        <item name="colorControlActivated">@color/mediumYellow</item>
    </style>
</resources>
